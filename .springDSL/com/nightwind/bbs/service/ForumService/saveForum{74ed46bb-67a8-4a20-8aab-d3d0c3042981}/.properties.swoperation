<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Forum entity" internalId="74ed46bb-67a8-4a20-8aab-d3d0c3042981"><inputs name="forum"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Forum.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Forum existingForum = forumDAO.findForumByPrimaryKey(forum.getId());&#xA;&#xA;if (existingForum != null)&#x9;{&#xA;&#x9;if (existingForum != forum) {&#xA;&#x9;&#x9;&#x9;existingForum.setId (forum.getId());&#xA;&#x9;&#x9;&#x9;existingForum.setTitle (forum.getTitle());&#xA;&#x9;&#x9;&#x9;existingForum.setDescription (forum.getDescription());&#xA;&#x9;}&#xA;&#x9;forum = forumDAO.store(existingForum);&#xA;}&#xA;else&#x9;{&#xA;&#x9;forum = forumDAO.store(forum);&#xA;}&#xA;forumDAO.flush();&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
