<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing User entity" internalId="f77c73dc-b85a-4765-815b-d3096b882724"><inputs name="forumthread_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_user_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="forumthread"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/ForumThread.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;ForumThread forumthread = forumThreadDAO.findForumThreadByPrimaryKey(forumthread_id, -1, -1);&#xA;&#x9;&#x9;User related_user = userDAO.findUserByPrimaryKey(related_user_id, -1, -1);&#xA;&#x9;&#xA;forumthread.setUser(null);&#xA;related_user.getForumThreads().remove(forumthread);&#x9;&#x9;&#xA;&#x9;&#x9;forumthread = forumThreadDAO.store(forumthread);&#xA;&#x9;&#x9;forumThreadDAO.flush();&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;related_user = userDAO.store(related_user);&#xA;&#x9;&#x9;userDAO.flush();&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;userDAO.remove(related_user);&#xA;&#x9;&#x9;userDAO.flush();&#xA;&#xA;return forumthread;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
