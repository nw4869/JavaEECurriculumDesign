<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Comment entity" internalId="60839500-d792-4f5f-ac25-37f8e40bc06e"><inputs name="forumthread_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_comments_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="forumthread"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/ForumThread.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;Comment related_comments = commentDAO.findCommentByPrimaryKey(related_comments_id, -1, -1);&#xA;&#x9;&#xA;&#x9;&#x9;ForumThread forumthread = forumThreadDAO.findForumThreadByPrimaryKey(forumthread_id, -1, -1);&#xA;&#x9;&#xA;related_comments.setForumThread(null);&#xA;forumthread.getComments().remove(related_comments);&#x9;&#x9;&#xA;&#x9;&#xA;&#x9;&#x9;commentDAO.remove(related_comments);&#xA;&#x9;&#x9;commentDAO.flush();&#xA;&#x9;&#x9;&#xA;&#xA;return forumthread;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
