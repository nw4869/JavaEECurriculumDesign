<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing Forum entity" internalId="34641b1d-a533-4016-90d2-28c8c1afcf04"><inputs name="forumthread_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_forum_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="forumthread"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/ForumThread.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;ForumThread forumthread = forumThreadDAO.findForumThreadByPrimaryKey(forumthread_id, -1, -1);&#xA;&#x9;&#x9;Forum related_forum = forumDAO.findForumByPrimaryKey(related_forum_id, -1, -1);&#xA;&#x9;&#xA;forumthread.setForum(null);&#xA;related_forum.getForumThreads().remove(forumthread);&#x9;&#x9;&#xA;&#x9;&#x9;forumthread = forumThreadDAO.store(forumthread);&#xA;&#x9;&#x9;forumThreadDAO.flush();&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;related_forum = forumDAO.store(related_forum);&#xA;&#x9;&#x9;forumDAO.flush();&#xA;&#x9;&#x9;&#xA;&#x9;&#x9;forumDAO.remove(related_forum);&#xA;&#x9;&#x9;forumDAO.flush();&#xA;&#xA;return forumthread;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
