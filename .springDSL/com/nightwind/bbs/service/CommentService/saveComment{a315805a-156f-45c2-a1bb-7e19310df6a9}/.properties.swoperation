<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing Comment entity" internalId="a315805a-156f-45c2-a1bb-7e19310df6a9"><inputs name="comment"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/Comment.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="Comment existingComment = commentDAO.findCommentByPrimaryKey(comment.getId());&#xA;&#xA;if (existingComment != null)&#x9;{&#xA;&#x9;if (existingComment != comment) {&#xA;&#x9;&#x9;&#x9;existingComment.setId (comment.getId());&#xA;&#x9;&#x9;&#x9;existingComment.setTitle (comment.getTitle());&#xA;&#x9;&#x9;&#x9;existingComment.setContent (comment.getContent());&#xA;&#x9;&#x9;&#x9;existingComment.setCreateTime (comment.getCreateTime());&#xA;&#x9;}&#xA;&#x9;comment = commentDAO.store(existingComment);&#xA;}&#xA;else&#x9;{&#xA;&#x9;comment = commentDAO.store(comment);&#xA;}&#xA;commentDAO.flush();&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
