<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Save an existing User entity" internalId="b9649ed0-e1bd-478d-895b-f3f534d6de37"><inputs name="user"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/User.datatype#/"/></dataType></inputs><implementation xsi:type="bean:TextualMethodImplementation" text="User existingUser = userDAO.findUserByPrimaryKey(user.getId());&#xA;&#xA;if (existingUser != null)&#x9;{&#xA;&#x9;if (existingUser != user) {&#xA;&#x9;&#x9;&#x9;existingUser.setId (user.getId());&#xA;&#x9;&#x9;&#x9;existingUser.setUsername (user.getUsername());&#xA;&#x9;&#x9;&#x9;existingUser.setPassword (user.getPassword());&#xA;&#x9;&#x9;&#x9;existingUser.setEmail (user.getEmail());&#xA;&#x9;&#x9;&#x9;existingUser.setAvatar (user.getAvatar());&#xA;&#x9;&#x9;&#x9;existingUser.setMemberTitle (user.getMemberTitle());&#xA;&#x9;&#x9;&#x9;existingUser.setSignature (user.getSignature());&#xA;&#x9;&#x9;&#x9;existingUser.setEnabled (user.getEnabled());&#xA;&#x9;&#x9;&#x9;existingUser.setCreateTime (user.getCreateTime());&#xA;&#x9;}&#xA;&#x9;user = userDAO.store(existingUser);&#xA;}&#xA;else&#x9;{&#xA;&#x9;user = userDAO.store(user);&#xA;}&#xA;userDAO.flush();&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
