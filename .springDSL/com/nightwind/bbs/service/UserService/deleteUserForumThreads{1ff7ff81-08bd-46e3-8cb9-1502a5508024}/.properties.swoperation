<skyway:SkywayServiceOperation xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:bean="model.bean" xmlns:skyway="model.service.skyway" xmlns:type="model.type" description="Delete an existing ForumThread entity" internalId="1ff7ff81-08bd-46e3-8cb9-1502a5508024"><inputs name="user_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><inputs name="related_forumthreads_id"><dataType xsi:type="type:PrimitiveDataType" type="INTEGER"/></inputs><outputs name="user"><dataType xsi:type="type:ComplexDataType"><type href="../../../domain/User.datatype#/"/></dataType></outputs><implementation xsi:type="bean:TextualMethodImplementation" text="&#x9;&#x9;ForumThread related_forumthreads = forumThreadDAO.findForumThreadByPrimaryKey(related_forumthreads_id, -1, -1);&#xA;&#x9;&#xA;&#x9;&#x9;User user = userDAO.findUserByPrimaryKey(user_id, -1, -1);&#xA;&#x9;&#xA;related_forumthreads.setUser(null);&#xA;user.getForumThreads().remove(related_forumthreads);&#x9;&#x9;&#xA;&#x9;&#xA;&#x9;&#x9;forumThreadDAO.remove(related_forumthreads);&#xA;&#x9;&#x9;forumThreadDAO.flush();&#xA;&#x9;&#x9;&#xA;&#xA;return user;&#xA;"/><modifiers>PUBLIC</modifiers></skyway:SkywayServiceOperation>
